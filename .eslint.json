{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:promise/recommended",
      "standard",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "node/no-unsupported-features/es-syntax": [
        "error",
        { "version": ">=10.0.0", "ignores": ["modules"] }
      ]
    }
  }
  